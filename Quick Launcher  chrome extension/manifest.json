{
    "manifest_version": 2,
    "name": "Quick Launcher",
    "description": "Quick launch favourite",
    "version": "1.0.0",
    "icons": {"128": "icon_128.jpg"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}